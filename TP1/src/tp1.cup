package lexsyn;

import java_cup.runtime.*;

parser code {:
	public static void main(String args[]) throws Exception {
		System.out.println("-----------------");
		parser myParser = new parser(new Yylex(System.in));
		myParser.parse();
		System.out.print("Accepted");
	}
:};
terminal LFPAREN, RTPAREN, SCOLON, BEGIN, END, PROGRAM;
terminal INTT, CHART, REALT;
terminal INT,REAL,CHAR,BOOLEAN;
terminal ASSIGN,COLON,COMMA;
terminal IF, ELSE,THEN,UNTIL,REPEAT;
terminal READ,WRITE;
terminal ID,VALUE,REFERENCE,PROCEDURE;
terminal NOT,FALSE,TRUE;

terminal LT,LE,GT,GE,EQ,NQ;
terminal PLUS, MINUS, MUL, DIV,OR,MOD,AND;

non terminal programI,program, decl_list, decl, dcl_var, indent_list,type;
non terminal tipo_retornado,corpo,id_return,espec_parametros; 
non terminal lista_de_parametros,parametro,modo,compound_stmt,stmt_list;
non terminal assign_stmt, if_stmt, cond,repeat_stmt, read_stmt,stmt;
non terminal write_stmt,expr_list,expr, Simple_expr, term,factor_a;
non terminal factor, function_ref_par;
non terminal constant, boolean_constant,dcl_proc;

precedence left PLUS, MINUS, MUL, DIV, ELSE;


programI ::= program:p {:
	System.out.println("-----------");
	String regras = new String("programI -> program\n" + p);
	System.out.print(regras);
:};
program ::= PROGRAM ID SCOLON decl_list:dl compound_stmt:cs {:
	String RESULTADO = new String("program -> ID decl_list compound_stmt\n" + dl + cs);

:};
decl_list ::= decl_list:dl SCOLON  decl:dc {:
		String RESULTADO = new String("decl_list -> decl_list ; decl\n" + dl+ dc);
	:}
	| decl:dc {:
		String RESULTADO = new String("decl_list -> decl\n" + dc);
	:};
decl ::= dcl_var:dv {:
		String RESULTADO = new String("decl -> decl_var\n" + dv);	
	:}
	| dcl_proc:dp {:
		String RESULTADO = new String("decl -> decl_proc\n" + dp);
	:};
dcl_var ::= indent_list:il COLON type:t {:
	String RESULTADO = new String("dcl_var -> type\n"+il+t);
:};
 indent_list ::= indent_list:il COMMA 	ID {:
		String RESULTADO =  new String(" ->   : id\n" + il);
	:}	|	ID {:
			String RESULTADO = new String(" -> id\n");	
	:};		
type ::= INT {:
		String RESULTADO = new String("type -> integer\n");
	:}	
	| REAL {:
		String RESULTADO = new String("type -> real\n");	
	:}
	| BOOLEAN {:
		String RESULTADO = new String("type -> boolean\n");
	:}
	| CHAR {:
		String RESULTADO = new String("type -> char\n");
	:};
dcl_proc ::= tipo_retornado:tr PROCEDURE ID espec_parametros:ep corpo:c {:
		String RESULTADO = new String("dcl_proc -> tipo_retornado procedure id espec_parametros\n" +tr+ep+c);
	:};	 

tipo_retornado ::= INT {:
		String RESULTADO = new String("type -> integer\n");
	:}	
	| REAL {:
		String RESULTADO = new String("type -> real\n");	
	:}
	| BOOLEAN {:
		String RESULTADO = new String("type -> boolean\n");
	:}
	| CHAR {:
		String RESULTADO = new String("type -> char\n");
	:}
	| /* vazio */ {:
			String RESULTADO = new String("tipo_retornado -> \n\n");	
	:};
corpo ::= COLON decl_list:dl SCOLON compound_stmt:cs id_return:ir {:
		String RESULTADO = new String("corpo -> : decl_list ; compound_stmt id_return\n" + dl+cs+ir);
	:}
	| /* vazio */ {:
		String RESULTADO = new String("corpo -> \n\n");
	:};
id_return ::= ID {:
		String RESULTADO = new String("id_return -> id\n");
	:}
	| 	/* vazio */ {:
		String RESULTADO = new String("id_return -> \n\n");
	:};

espec_parametros ::= LFPAREN lista_de_parametros:lp RTPAREN {:
		String RESULTADO = new String("espec_parametros -> ( lista_de_parametros )\n"+ lp);
	:};
lista_de_parametros ::= parametro:p {:
		String RESULTADO = new String("lista_de_parametros -> parametro\n"+ p);
	:}
	| lista_de_parametros:lp COMMA parametro:p {:
		String RESULTADO = new String("lista_de_parametros -> lista_de_parametros , parametro\n"+ lp+ p);
	:}; 
parametro ::= modo:m type:t COLON ID {:
		String RESULTADO = new String("parametro -> modo type : id\n"+m+t);
	:};
modo ::= VALUE {:
		String RESULTADO = new String("modo -> value\n");
	:}
	| REFERENCE {:
		String RESULTADO = new String("modo -> reference\n");
	:};
compound_stmt ::= BEGIN stmt_list:sl END {:
		String RESULTADO = new String("compound_stmt -> begin stmt_list end\n"+sl);
	:};
stmt_list ::= stmt_list:sl SCOLON stmt:s {:
		String RESULTADO = new String("stmt_list -> stmt_list ; stmt\n"+sl+s);
	:} 
	| stmt:s {:
		String RESULTADO = new String("stmt_list -> stmt\n"+s);
	:};
stmt ::= assign_stmt:as {:
		String RESULTADO = new String("stmt -> assign_stmt\n"+as);
	:}
	| if_stmt:is{:
		String RESULTADO = new String("stmt -> if_stmt\n"+is);
	:}
	| repeat_stmt:rs {:
		String RESULTADO = new String("stmt -> repeat_stmt\n"+ rs);
	:}
	| read_stmt:rds {:
		String RESULTADO = new String("stmt -> read_stmt\n"+rds);
	:}
	| write_stmt:ws {:
		String RESULTADO = new String("stmt -> write_stmt\n"+ ws);
	:}
	| compound_stmt:cs {:
		String RESULTADO = new String("stmt -> compound_stmt\n"+cs);
	:}
	| function_ref_par:frp {:
		String RESULTADO = new String("stmt -> function_ref_par\n"+frp);
	:};
assign_stmt ::= ID ASSIGN expr:e {:
		String RESULTADO = new String("assign_stmt -> id := expr\n"+e);
	:};
if_stmt ::= IF cond:c THEN stmt:s {:
		String RESULTADO = new String("if_stmt -> if cond then stmt\n"+c+s);
	:}
	| IF cond:c THEN stmt:s1 ELSE stmt:s2 {:
			String RESULTADO = new String("if_stmt -> if cond then stmt else stmt\n"+c+s1+s2);
	:};
cond ::= expr:e {:
		String RESULTADO = new String("cond -> expr\n"+e);
	:};
repeat_stmt ::= REPEAT stmt_list:st UNTIL expr:e {:
		String RESULTADO = new String("repeat_stmt -> repeat stmt_list until expr\n"+st+e);
	:};
read_stmt ::= READ LFPAREN :il RTPAREN {:
		String RESULTADO = new String("read_stmt -> read(  )\n"+il);
	:};
write_stmt ::= WRITE LFPAREN expr_list:el RTPAREN	{:
		String RESULTADO = new String("write_stmt -> ( expr_list )\n"+el);
	:};
expr_list ::= expr:e {:
		String RESULTADO = new String("expr_list -> expr\n"+e);
	:}
	| expr_list:el COMMA expr:e {:
		String RESULTADO = new String("expr_list -> expr_list , expr\n"+el+e);
	:};
expr ::= Simple_expr:se {:
		String RESULTADO = new String("expr -> Simple_expr\n"+se);
	:}
	| Simple_expr:se1 LE Simple_expr:se2 {:
		String RESULTADO = new String("expr -> Simple_expr LE Simple_expr \n"+se1+se2);
	:} 
	| Simple_expr:se1 LT Simple_expr:se2 {:
		String RESULTADO = new String("expr -> Simple_expr LT Simple_expr \n"+se1+se2);
	:} 
	| Simple_expr:se1 GE Simple_expr:se2 {:
		String RESULTADO = new String("expr -> Simple_expr GE Simple_expr \n"+se1+se2);
	:} 
	| Simple_expr:se1 GT Simple_expr:se2 {:
		String RESULTADO = new String("expr -> Simple_expr GT Simple_expr \n"+se1+se2);
	:} 
	| Simple_expr:se1 EQ Simple_expr:se2 {:
		String RESULTADO = new String("expr -> Simple_expr EQ  Simple_expr \n"+se1+se2);
	:} 
	| Simple_expr:se1 NQ Simple_expr:se2 {:
		String RESULTADO = new String("expr -> Simple_expr NQ Simple_expr \n"+se1+se2);
	:}; 

Simple_expr ::= term:t {:
			String RESULTADO = new String("Simple_expr -> term\n"+t);
	:}
	| Simple_expr:se PLUS term:t {:
		String RESULTADO = new String("Simple_expr -> Simple_expr + term"+se+t);
	:}
	| Simple_expr:se MINUS term:t {:
		String RESULTADO = new String("Simple_expr -> Simple_expr - term"+se+t);
	:}
	| Simple_expr:se OR term:t {:
		String RESULTADO = new String("Simple_expr -> Simple_expr or term"+se+t);
	:};
term ::= factor_a:fa {:
		String RESULTADO = new String("term -> factor_a\n"+fa);
	:}
	| term:t MUL factor_a:fa {:
		String RESULTADO = new String("term -> term * factor_a\n"+t+fa);
	:}
	| term:t DIV factor_a:fa {:
		String RESULTADO = new String("term -> term / factor_a\n"+t+fa);
	:}
	| term:t AND factor_a:fa {:
		String RESULTADO = new String("term -> term and factor_a\n"+t+fa);
	:}
	| term:t MOD factor_a:fa {:
		String RESULTADO = new String("term -> term mod factor_a\n"+t+fa);
	:};
	
factor_a ::= MINUS factor:f {:
	String RESULTADO = new String("factor_a -> - factor\n"+f);
:}
| factor:f {:
	String RESULTADO = new String("factor_a -> factor\n"+f);
:};

factor ::= ID {:
	String RESULTADO = new String("factor -> id");
:}
| constant:c {:
	String RESULTADO = new String("factor -> constant\n");
:} 
| LFPAREN expr:e RTPAREN {:
	String RESULTADO = new String("factor -> ( expr )\n"+e);
:}
| NOT factor:f {:
	String RESULTADO = new String("factor -> not factor\n"+f);
:}
| function_ref_par:frp {:
	String RESULTADO = new String("factor -> function_ref_par\n"+frp);
:};
/*Simple_variable_or_proc ::= ID {:
	String RESULTADO = new String("Simple_variable_or_proc -> id\n");
:};
variable ::= ID {:
	String RESULTADO = new String("factor -> Simple_variable_or_proc\n"+svp);
:};*/

constant ::= INTT {:
	String RESULTADO = new String("constant -> integer_constant\n");
:}
| REALT {:
	String RESULTADO = new String("constant -> real_constant\n");
:}
| boolean_constant:bc {:
	String RESULTADO = new String("constant -> boolean_constant\n"+bc);
:}
| CHART {:
	String RESULTADO = new String("constant -> char_constant\n");
:};

boolean_constant ::= FALSE {:
	String RESULTADO = new String("boolean_constant -> false\n");
:}
| TRUE {:
	String RESULTADO = new String("boolean_constant -> true\n");
:};

